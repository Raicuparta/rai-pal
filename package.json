{
	"name": "rai-mods",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "tauri dev",
		"build": "tauri build",
		"build-debug": "cross-env CARGO_TARGET_DIR=target/debug/standalone-build tauri build --debug --no-bundle",
		"vite": "vite",
		"tauri": "tauri",
		"update-icons": "tauri icon backend/graphics/icon-source.png",
		"frontend:build": "tsc && vite build",
		"frontend:dev": "vite",
		"lint": "prettier ./frontend --write && eslint . --fix",
		"lint-only": "eslint eslint.config.mjs --fix",
		"find-unused": "cd backend && del index.scip &&  cargo workspace-unused-pub"
	},
	"prettier": {
		"useTabs": true,
		"singleAttributePerLine": true,
		"quoteProps": "as-needed"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"dependencies": {
		"@eslint/compat": "1.3.1",
		"@eslint/eslintrc": "3.3.1",
		"@eslint/js": "9.31.0",
		"@mantine/code-highlight": "8.1.3",
		"@mantine/core": "8.1.3",
		"@mantine/hooks": "8.1.3",
		"@mantine/notifications": "8.1.3",
		"@tabler/icons-react": "3.34.0",
		"@tauri-apps/api": "2.6.0",
		"@tauri-apps/cli": "2.6.2",
		"@tauri-apps/plugin-dialog": "2.3.0",
		"@tauri-apps/plugin-os": "2.3.0",
		"@tauri-apps/plugin-process": "2.3.0",
		"@tauri-apps/plugin-shell": "2.3.0",
		"@tauri-apps/plugin-store": "2.3.0",
		"@tauri-apps/plugin-updater": "2.9.0",
		"@types/node": "24.0.13",
		"@types/react-dom": "19.1.6",
		"@types/react": "19.1.8",
		"@typescript-eslint/eslint-plugin": "8.36.0",
		"@typescript-eslint/parser": "8.36.0",
		"@vitejs/plugin-react": "4.6.0",
		"babel-plugin-react-compiler": "19.1.0-rc.2",
		"cross-env": "7.0.3",
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-react-compiler": "19.1.0-rc.2",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.4.20",
		"eslint-plugin-react": "7.37.5",
		"eslint": "9.31.0",
		"globals": "16.3.0",
		"jotai": "2.12.5",
		"postcss-preset-mantine": "1.18.0",
		"postcss-simple-vars": "7.0.1",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"react-dom": "19.1.0",
		"react-virtuoso": "4.13.0",
		"react": "19.1.0",
		"typescript": "5.8.3",
		"vite": "7.0.4"
	}
